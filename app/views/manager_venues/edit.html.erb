<%= form_for(@venue) do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="col-md-12">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control", :disabled => true %>
        </div>
        <div class="col-md-12">
          <%= f.label :status, t(".status") %>
          <%= f.select :status, options_for_select(edit_venue_status.collect{|u| [t(u[:name]), {class: "dropdown-menu show"}, u[:id]]}), {} %>
        </div>
        <%= f.submit "Cập nhật", class:"btn btn-primary" %>
      </div>
  </div>
<% end %>

